{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "strict": true,
        "target": "es2020",
        "module": "es2022",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "paths": {
            "pagefindService": [
                "./types/index"
            ],
            "pagefindInternal": [
                "./types/internal"
            ],
        },
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "include": [
        "lib/**/*"
    ]
}